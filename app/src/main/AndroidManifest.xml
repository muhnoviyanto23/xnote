<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.xnote">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Barang_terima"
            android:label="@string/title_activity_barang_terima"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Barang_distribusi"
            android:label="@string/title_activity_barang_distribusi"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Cetak_laporan"
            android:label="@string/title_activity_cetak_laporan"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>